{
    "extends": [
        "airbnb",
        "prettier",
        "plugin:node/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        //"prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "prefer-const": "off", 
        "func-names": "warn",
        "no-underscore-dangle": "off",
        "no-restricted-syntax": "off"
    }
}
